{
  "name": "@wally-ui/cli",
  "version": "1.0.6",
  "description": "A modern Angular component library CLI - Copy & paste beautiful components",
  "main": "dist/index.js",
  "bin": {
    "wally": "dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "templates/**/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "npm run build && node dist/cli.js",
    "test": "echo 'Tests temporarily disabled - will be configured soon' && exit 0",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "angular",
    "cli",
    "component-generator",
    "tailwind",
    "typescript",
    "standalone-components",
    "signals",
    "modern-angular"
  ],
  "author": "Walisson Ferreira",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/WalissonCF/test-cli.git"
  },
  "homepage": "https://github.com/WalissonCF/test-cli#readme",
  "bugs": {
    "url": "https://github.com/WalissonCF/test-cli/issues"
  },
  "engines": {
    "node": ">=18.13.0"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "chalk": "^5.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^14.0.0",
    "fs-extra": "^11.3.1",
    "glob": "^11.0.3",
    "inquirer": "^12.9.4",
    "ora": "^8.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.1",
    "jest": "^30.1.3",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  }
}
